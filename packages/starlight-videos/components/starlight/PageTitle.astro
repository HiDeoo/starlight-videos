---
import type { Props } from '@astrojs/starlight/props'

import { isEntryWithVideo } from '../../libs/video'

import DraftContentNotice from './DraftContentNotice.astro'
import FallbackContentNotice from './FallbackContentNotice.astro'

const { entry, isFallback } = Astro.props
---

{entry.data.draft && <DraftContentNotice {...Astro.props} />}
{isFallback && <FallbackContentNotice {...Astro.props} />}

{isEntryWithVideo(entry) && <div>video</div>}
